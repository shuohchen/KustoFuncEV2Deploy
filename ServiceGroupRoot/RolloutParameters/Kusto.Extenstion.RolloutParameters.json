    {
      "Name": "CreateFunction",
      "Type": "Microsoft.Kusto/KustoExtension",
      "Version": "2019-04-01",
      "ConnectionProperties": {
        "MaxExecutionTime": "PT15M",
        "Authentication": {
          "Type": "AadApplicationAuthentication",
          "Reference": {
            "Provider": "AzureKeyVault",
            "Parameters": {
              "SecretId": "https://kustofunctestkeyvault.vault.azure.net/secrets/appsecret/a4a8ed5f6a8143ba866b6447878d2d73"
            }
          },
          "Properties": {
            "ApplicationId": "1fbca744-ecfa-4f18-852e-740b7572c958",
