          }
        }
      },
      "payloadProperties": {
        "ConnectionString": {
          "Value": "https://kustofunctestcluster.japaneast.kusto.windows.net"
        },
        "KqlSasUri": {
          "reference": {
            "path": "Parameters\\myFunc.kql",
            "enableScopeTagBindings": "false"
